{
  "name": "@shoutkol/pumble-mcp-server",
  "version": "1.0.2",
  "description": "MCP server for Pumble",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pumble-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "nock": "^13.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "undici": "^6.19.0",
    "vitest": "^1.2.0"
  }
}
